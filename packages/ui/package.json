  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./react": "./dist/react/exports.js",
    "./vue": "./dist/vue/exports.js",
    "./theme": "./dist/lib/theme/theme.js",
    "./tailwind.preset": "./dist/tailwind.preset.mjs",
