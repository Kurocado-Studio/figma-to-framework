    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.3",
    "@vue/compiler-sfc": "^3.5.14",
    "copyfiles": "^2.4.1",
    "eslint": "^9.28.0",
    "postcss": "^8.5.3",
